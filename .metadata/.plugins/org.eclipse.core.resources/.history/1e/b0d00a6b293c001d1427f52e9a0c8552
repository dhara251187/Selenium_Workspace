package tests;

public class DataTypesTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int i=10;
		System.out.println(i);
		i=20;
		System.out.println(i);
		i=i+10;
		System.out.println(i);
		i=i*2;
		System.out.println(i);
		System.out.println(i*2);
		System.out.println(i);
		
		System.out.println(i++);
		System.out.println(i);
		
		//double
		double d=11.12;
		System.out.println(d);
		System.out.println(d*2);
		
	}

}
